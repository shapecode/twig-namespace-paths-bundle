{
    "name": "shapecode/twig-namespace-paths-bundle",
    "description": "",
    "keywords": [
        "symfony",
        "shapecode",
        "twig",
        "template",
        "namespace",
        "path",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/twig-namespace-paths-bundle",
    "support": {
        "email": "support@shapecode.de",
        "issues": "https://github.com/shapecode/twig-namespace-paths-bundle/issues",
        "source": "https://github.com/shapecode/twig-namespace-paths-bundle/releases",
        "wiki": "https://github.com/shapecode/twig-namespace-paths-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/twig-bundle": "~2.8|~3.0|~4.0",

        "twig/twig": "~1.34|~2.0"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\TwigNamespacePathsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\TwigNamespacePathsBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
